    {
        "version": 2,
        "builds": [
            {
                "src": "index.js",
                "use": "@vercel/node"
            }
        ],
        "routes": [
            {
                "src": "/(.*)",
                "dest": "/index.js"
            },
            {
                "src": "/api/auth/(.*)",
                "dest": "/routes/auth.js"
            },
            {
                "src": "/api/notes/(.*)",
                "dest": "/routes/notes.js"
            }
        ]
    }